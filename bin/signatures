#!/usr/bin/env bash
#
# bin/signatures -cp ...
#

source /r/jbash/jbash

_signatures_cmd()
{
  local jbash_flags jbash_args jbash_classpath="."

  jbash_program=$(basename "$0")
  jbash_help="Print each signature in the classes found on the given classpath."
  _jbash_by_ref "$@"

  cp-signatures "$jbash_classpath"
}

_signatures_cmd "$@"
